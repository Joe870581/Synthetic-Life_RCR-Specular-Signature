const Equation = ({
  children,
  inline,
}: {
  children: React.ReactNode;
  inline?: boolean;
}) => {
  const Comp = inline ? 'span' : 'div';
  const className = inline
    ? "inline-block font-code bg-muted/80 px-2 py-1 rounded-md text-accent-foreground mx-1"
    : "font-code bg-muted text-center p-4 my-4 rounded-lg text-lg overflow-x-auto";
  
  return (
    <Comp className={className}>
      {children}
    </Comp>
  );
};

